/*--------------------------------*- C++ -*----------------------------------*\
| OpenFOAM Engineering Manual                                                 |
| Specific dissipation rate                                                   |
\*---------------------------------------------------------------------------*/

FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}

dimensions      [0 0 -1 0 0 0 0];

// Conservative initial value
internalField   uniform 10;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 10;
    }

    outlet
    {
        type            zeroGradient;
    }

    walls
    {
        // Automatic near-wall omega handling
        type            omegaWallFunction;
        value           uniform 10;
    }

    symmetry
    {
        type            symmetryPlane;
    }

    frontAndBack
    {
        type            empty;
    }
}
